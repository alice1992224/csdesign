    <script>
	$('input[name=candidate_img]').change(function () {
		readURL(this);
	});
	
	function readURL(input) {
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            
            reader.onload = function (e) {
                $('#viewImg').attr('src', e.target.result);
            }
            
            reader.readAsDataURL(input.files[0]);
        }
    }
	
	$(".portfolio-link button").click(function()
	{
		var title = $('#'+this.value+" .modal-body h2").html();
		$("#votepage h2").html(title);
		$("#votepage .candidate_id").val(this.value);
		$("#votecancel").val(this.value);
	})
	
	$('#votecancel').click(function()
	{
		$("#"+$("#votecancel").val()+" .close-modal").trigger('click');
	});
	
	$('form[name="sentMessage"]').submit(function () {
		var student_id = $.trim($("input[name='student_id']").val());
		var candidate_id = $.trim($("input[name='candidate_id']").val());
		var phone = $.trim($("input[name='phone']").val());
		var email = $.trim($("input[name='email']").val());
		var position = $("#position option:selected").val();
		
		var found = false;
			$.ajax({
				async: false,
				type: 'POST',
				url: "votecheck.php",
				data: { position: position, student_id: student_id, phone: phone, email: email, candidate_id: candidate_id },
				success: function(data) {
					if(data == 'true')
					{
						alert('投票成功 感謝您的參與');
						found = true;
					}
					if(data == 'false')
					{
						alert("請檢查學號 / 工號 / 身分證字號\n\n在校生請輸入「學號」\n現職教職員請輸入「工號」\n校友請輸入「身分證字號」");
						found = false;
					}
					if(data == 'exsit')
					{
						alert("你已經投過囉!!");
						found = false;
					}
				}
			});
			
			if(found)
			{
			  return true;	
			}
			else
			{
			  return false;
			}
			
			event.preventDefault();
	});
	
	$('form[name="sendupload"]').submit(function () {
		var candidate_ai = $.trim($("input[name='candidate_ai']").val());
		var candidate_registerform = $.trim($("input[name='candidate_registerform']").val());
		var candidate_explanation = $.trim($("input[name='candidate_explanation']").val());
		var candidate_consent = $.trim($("input[name='candidate_consent']").val());
		var found = false;
			if(candidate_ai.length > 0 &&
			   candidate_registerform.length > 0 &&
			   candidate_explanation.length > 0 &&
			   candidate_consent.length > 0)
			{
			   return true;
			}
			else
			{
			  var error_msg = "請上傳:\n\n";
			  
			  if(candidate_ai.length<=0)
			  {
			    error_msg += "AI檔\n";
			  }
			  if(candidate_registerform.length<=0)
			  {
			    error_msg += "活動報名表\n";
			  }
			  if(candidate_explanation.length<=0)
			  {
			    error_msg += "活動作品圖片及理念說明\n";
			  }
			  if(candidate_consent.length<=0)
			  {
			    error_msg += "活動同意書\n";
			  }
			  alert(error_msg);
			  return false;
			}
			//return false;
	});
	</script>
