                            <div class="form-group col-xs-12 floating-label-form-group controls">
                                <label>科系班別 / 服務單位 department/service units</label>
                                <input name="department" type="text" autocomplete="off" class="form-control" placeholder="科系班別 / 服務單位 department/service units" id="name" required data-validation-required-message="請輸入科系班別 department">
